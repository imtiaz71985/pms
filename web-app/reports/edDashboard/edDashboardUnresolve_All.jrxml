	<queryString>
		<![CDATA[SELECT ed.id ,ed.version,ed.service_id,pss.name, edi.issue_name AS issueName ,ed.is_resolve,edi.id AS issueId
        ,ed.followup_month_for, CASE WHEN ed.is_followup=1 THEN DATE_FORMAT(ed.followup_month_for, '%M %Y')
        ELSE DATE_FORMAT(ed.month_for, '%M %Y')END AS issuedMonthStr
        ,CASE WHEN ed.is_followup=1 THEN 'Follow-up'
        WHEN ed.is_resolve = 1 THEN 'Resolved' ELSE 'New' END AS issue_status
        ,ed.description,ed.ed_advice AS edAdvice,ed.remarks
        FROM  ed_dashboard ed INNER JOIN ed_dashboard_issues edi ON ed.issue_id=edi.id
        LEFT JOIN pm_service_sector pss ON ed.service_id=pss.id
        WHERE ed.month_for =DATE($P{month})AND ed.service_id IN (
           SELECT DISTINCT(service_id) FROM pm_mcrs_log
                  WHERE is_submitted_db = TRUE AND MONTH = MONTH($P{month}) AND YEAR=YEAR($P{month})
            )
        ORDER BY ed.issue_id,ed.month_for;]]>
	</queryString>
